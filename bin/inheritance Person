class Car {
  String color;
  int year;
  Car(this.color, this.year);

  void speed(){
    print('color: $color and year: $year');
  }
}

class Toyta extends Car{
  String moden;
  double prize;
  Toyta(String color, int year, this.moden, this.prize): super(color,year);

  void speeds (){
    print("this is car model: $moden and prize: $prize");
  }
}

import 'child class.dart';

void main() {
  var toyta = Toyta('red', 2014, 'xz 19', 120000000);
  toyta.speed();
  toyta.speeds();
}
